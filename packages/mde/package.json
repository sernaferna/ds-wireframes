{
  "name": "@devouringscripture/mde",
  "version": "1.1.1",
  "description": "Markdown Editor component for the Devouring Scripture app",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "/lib"
  ],
  "keywords": [
    "react",
    "bootstrap",
    "react-bootstrap",
    "markdown"
  ],
  "author": "sernaferna",
  "license": "ISC",
  "scripts": {
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build": "npm run build:esm & npm run build:cjs",
    "test": "jest --watchAll --no-cache"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "dependencies": {
    "@devouringscripture/common": "*",
    "js-file-download": "^0.4.12",
    "marked": "^4.2.4",
    "react-hotkeys": "^2.0.0"
  },
  "devDependencies": {
    "@types/marked": "^4.0.7",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "bootstrap": "^5.2.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@types/react": ">=18.0.26",
    "@types/react-dom": "18.0.9",
    "bootstrap": ">=5.2.3",
    "react": ">=18.2.0",
    "react-bootstrap": ">=2.7.0",
    "react-bootstrap-icons": ">=1.10.2",
    "react-dom": ">=18.2.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
