{
  "name": "@devouringscripture/refparse",
  "version": "1.0.0",
  "description": "Utility for parsing and working with bible references",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "watch": "tsc --build --watch",
    "test": "jest --watchAll --no-cache"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "keywords": [
    "typescript"
  ],
  "author": "sernaferna",
  "license": "ISC",
  "dependencies": {
    "bible-passage-reference-parser": "^2.0.1",
    "bible-reference-formatter": "^1.0.1"
  }
}
